{
  "name": "react-gh-pages",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://paigeribera.github.io/cs340-final-project",
  "dependencies": {
    "axios": "^0.18.0",
    "gh-pages": "^1.1.0",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.2.11",
    "react-scripts": "1.1.1",
    "shortid": "^2.2.8",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build-css":
      "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css":
      "npm run build-css && npm run build-css -- --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0"
  }
}
